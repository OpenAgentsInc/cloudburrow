{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cloudburrow-worker",
  "main": "src/index.ts",
  "compatibility_date": "2025-10-28",
  "compatibility_flags": ["nodejs_compat"],
  "vars": {
    "TUNNEL_HOST_PREFIX": "cloudburrow-",
    "TUNNEL_HOST_SUFFIX": "openagents.com"
  },
  "routes": [
    { "pattern": "cloudburrow-broker.openagents.com/*", "zone_name": "openagents.com" }
  ]
}
